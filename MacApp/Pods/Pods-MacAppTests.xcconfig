PODS_ROOT = ${SRCROOT}/Pods
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
FRAMEWORK_SEARCH_PATHS = "$(inherited)" "$(SDKROOT)/Developer/Library/Frameworks" "$(DEVELOPER_LIBRARY_DIR)/Frameworks"
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/JSONKit" "${PODS_ROOT}/BuildHeaders/Kiwi"
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/JSONKit" "${PODS_ROOT}/Headers/Kiwi"
OTHER_LDFLAGS = -ObjC -framework SenTestingKit
ALWAYS_SEARCH_USER_PATHS = YES